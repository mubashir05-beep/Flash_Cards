{
    "builds": [
        {
            "src": "server/package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/server/(.*)",
            "dest": "/server/src/index.ts"
        },
        {
            "src": "/client/(.*)",
            "dest": "/client/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}
